# Production Environment Variables
# Copy this file to .env and fill in production values

# Database
MYSQL_ROOT_PASSWORD=your_secure_password_here
MYSQL_DATABASE=irctc_food_db

# JWT Secret (generate a secure 256-bit key)
JWT_SECRET=your_super_secure_jwt_secret_key_here_at_least_256_bits

# Razorpay (use live keys for production)
RAZORPAY_KEY_ID=your_live_razorpay_key_id
RAZORPAY_KEY_SECRET=your_live_razorpay_key_secret

# URLs
FRONTEND_URL=https://your-frontend-domain.com
BACKEND_URL=https://your-backend-domain.com
